pipeline {
    agent any
    stages {
        stage('Build image nginx') {
            steps {
                scripts {
                    sh """
                    docker build -t nginx:v1 .
                    """
                }
            }
        }

        stage('running image') {
            steps {
                scripts {
                    sh """
                    docker run -d --name nginx nginx:v1
                     """
                }
            }
        }
    }
}