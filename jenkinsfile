pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo hello world from build'
            }
        }
        stage('Test') {
            steps {
                sh 'echo hello world from test'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo hello world from Deploy'
            }
        }
    }
}
